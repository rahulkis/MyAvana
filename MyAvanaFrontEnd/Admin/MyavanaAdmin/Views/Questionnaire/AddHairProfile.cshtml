    var eduVideos = clients.GetEducationalVideos(Convert.ToInt32(HttpContextAccessor.HttpContext.Request.Cookies["UserId"]));
    var hhcpList = clients.GetHHCPList(Model.UserEmail).Result;
    Model.ModifiedBy = Convert.ToInt32(HttpContextAccessor.HttpContext.Request.Cookies["UserTypeId"]);
}
@{
    Layout = "_Layout";
}

